<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="root" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@popup.css" />
        <URL value="@../../../media/buttonStyles.css" />
        <URL value="@../../../media/fonts.css" />
    </stylesheets>
    <children>
        <Pane fx:id="drag" prefHeight="50.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <Text fx:id="title" fill="WHITE" layoutX="5.0" layoutY="5.0" strokeType="OUTSIDE" strokeWidth="0.0" textOrigin="TOP">
                <font>
                    <Font name="LIBRARY3AM" size="14.0" />
                </font>
            </Text>
        </Pane>
      <AnchorPane fx:id="contentPane" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0" />
      <ButtonBar fx:id="buttonBar" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="0.0">
         <padding>
            <Insets right="20.0" />
         </padding>
      </ButtonBar>
    </children>
</AnchorPane>
